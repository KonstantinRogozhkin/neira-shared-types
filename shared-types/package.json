{
  "name": "neira-shared-types",
  "version": "1.2.0",
  "description": "Общие типы для IPC контрактов между main и renderer процессами",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch"
  },
  "license": "GPL-3.0",
  "devDependencies": {
    "tsup": "^8.0.0",
    "typescript": "^5.8.3"
  },
  "peerDependencies": {
    "typescript": "^5.8.3"
  }
}
